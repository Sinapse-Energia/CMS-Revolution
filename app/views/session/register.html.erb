<body data-gr-c-s-loaded="true">
        <div class="login-container">
            <div class="left-section"></div>
            <div class="right-section">
                <div class="logo-section">
                    <a href="/"><img alt="logo" src="/assets/login-logo-ee82b9552305f2f1570dfdcd1abee04e2c61734fe756dd3dd40bd2a512b532e2.svg"></a>
                </div>

                <div class="form-section">

                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active">
                            <a role="tab" href="/session/index">Sign In</a>
                        </li>
                        <li role="presentation" class="">
                            <a role="tab" href="/session/register">Sign Up</a>
                        </li>
                    </ul>

                  
<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="signin">

    <form class="form-horizontal" id="new_user" accept-charset="UTF-8" method="post" action="/session/create_register" >
    
        <div class="form-group">
            <div class="col-xs-12">
                <input class="form-control email-filed" placeholder="First Name" type="text" name="user[first_name]" id="user_first_name">
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <input class="form-control email-filed" placeholder="Last Name" type="text" name="user[last_name]" id="user_last_name">
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <input class="form-control msg-filed" placeholder="Email Address" type="email" value="" name="user[email]" id="user_email">
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <input class="form-control password-filed" placeholder="Password" type="password" name="user[password]" id="user_password">
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                  <input class="form-control password-filed" placeholder="Confirm Password" type="password" name="user[password_confirmation]" id="user_password_confirmation">
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <div class="checkbox">
                    <input name="user[terms_of_service]" type="hidden" value="0"><input id="tc1" type="checkbox" value="1" name="user[terms_of_service]">
                    <label for="tc1">I accept <a target="_blank" href="/terms">Terms &amp; Conditions</a></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <button type="submit" class="btn btn-default login-btn">Sign Up</button>
            </div>
        </div>
    </form>
</div>
   </div>
 </div>
</div>
 </div>
</body>
